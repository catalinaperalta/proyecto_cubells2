<p id="notice"><%= notice %></p>

<h1>Listing Materia Alumnos</h1>

<table>
  <thead>
    <tr>
      <th>Materia</th>
      <th>Alumno</th>
      <th>Fecha</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @materia_alumnos.each do |materia_alumno| %>
      <tr>
        <td><%= materia_alumno.materia %></td>
        <td><%= materia_alumno.alumno %></td>
        <td><%= materia_alumno.fecha %></td>
        <td><%= link_to 'Show', materia_alumno %></td>
        <td><%= link_to 'Edit', edit_materia_alumno_path(materia_alumno) %></td>
        <td><%= link_to 'Destroy', materia_alumno, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Materia alumno', new_materia_alumno_path %>
