<p id="notice"><%= notice %></p>

<h1>Listing Impartes</h1>

<table>
  <thead>
    <tr>
      <th>Codma</th>
      <th>Nomina</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @impartes.each do |imparte| %>
      <tr>
        <td><%= imparte.codma %></td>
        <td><%= imparte.nomina %></td>
        <td><%= link_to 'Show', imparte %></td>
        <td><%= link_to 'Edit', edit_imparte_path(imparte) %></td>
        <td><%= link_to 'Destroy', imparte, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Imparte', new_imparte_path %>
